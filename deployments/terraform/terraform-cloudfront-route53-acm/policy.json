{
	"Version": "2012-10-17",
	"Statement": [
		{
		 	"Effect": "Allow",
	     	"Principal": "*",
			"Action": "s3:GetObject",
			"Resource": "arn:aws:s3:::socialcloudsync.com/*",
			"Condition": {
				"StringEquals": {
					"aws:SourceArn": "arn:aws:cloudfront::xxx:distribution/E2SZ49FXEKL75W"
				}
			}
		}
	]
}

